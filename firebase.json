{
  "hosting": {
    "public": "out",
    "site": "televerse",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/doc/custom-context",
        "destination": "https://github.com/xooniverse/televerse/wiki/Using-Custom-Context-with-Your-Bot"
      }
    ]
  }
}